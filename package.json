    "build:prd": "concurrently \"ng build --prod backend\" \"ng build frontend --configuration=production-de\" \"ng build --prod frontend\"",
    "test": "ng test",
    "lint": "./node_modules/.bin/nx workspace-lint && ng lint",
    "e2e": "ng e2e",
